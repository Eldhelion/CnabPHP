{
    "name": "eldhelion/cnab_php",
    "type": "library",
    "description": "Projeto para criar arquivos de remessas e processar arquivos de retorno no formato CNAB",
    "keywords": ["boleto", "remessa", "retorno"],
    "licence": "MIT",
    "authors": [
        {
            "name": "Suporte",
            "email": "suporte@aptor.com.br"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/php-code-coverage": "1.2.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "require": {
        "eldhelion/cnab_yaml": "master-dev",
        "php": ">=5.3.0",
        "mustangostang/spyc": "0.5.*"
    },
    "repositories": [
        {
            "type": "git",
            "url":  "https://github.com/eldhelion/cnab_yaml.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cnab\\": "src/Cnab"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
